<!-- ======= Schedule Meeting ======= -->
<div id="schedule_meeting" class="modal">
    <div class="row justify-content-center">
        <div class="col-xl-6 col-lg-8 col-sm-10">
            <div class="modal-content animate">
                <span onclick="close_popup('schedule_meeting')" class="close" title="Close">&times;</span>

                <div class="modal-header-container">
                    <h5>Schedule Meeting</h5>
                </div>
                <div class="modal-container">
                    <!-- Form -->
                    <form>
                        <p class="mb-3 text-gray-400" style="font-size:10pt">
                            <i class="fas fa-info-circle fa-x mr-2 "></i>
                            Only <span class="font-weight-bold text-orange">Qualified</span> applicants will recieve the meeting invitation
                        </p>
                        <!-- Meeting Date input-->
                        <div class="form-floating mb-3">
                            @*<input class="form-control gregorian-date-default" id="EDate" type="text" placeholder="Registration Close"  onpaste="validateDate(this,'open')" onfocusout="validateDate(this,'close','invalid-close-date','ODate')" />*@
                            <input class="form-control gregorian-date-default" id="MeetingDate" type="text" placeholder="Meeting Date" onpaste="vlidateEmpty(this)" onfocusout="vlidateEmpty(this)" />
                            <label for="MeetingDate">Meeting Date</label>
                            <div id="invalid-meeting-date" class="invalid-feedback">Required</div>
                        </div>

                        <!-- Meeting Message -->
                        <div class="form-floating mb-3">
                            <textarea style=" min-height:130px;" class="form-control" id="meetingMsg" placeholder="Message" onfocusout="vlidateEmpty(this)"></textarea>
                            <label for="ODate">Message</label>
                            <div id="invalid-open-date" class="invalid-feedback">Required</div>
                        </div>


                        <!-- Buttons -->
                        <div class="row justify-content-center mt-3 mb-3">
                            <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 mt-2">
