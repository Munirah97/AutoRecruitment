@{
    ViewBag.Title = "Campaigns";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h2 mb-0 text-gray-700">Campaigns</h1>
        <div class=" d-sm-inline-block">
            <button class="d-sm-inline-block btn btn-sm btn-primary shadow-sm" onclick="open_popup('importHRDF')">
                <i class="fas fa-file-import fa-sm text-white-50 mr-1"></i> Import HRDF
            </button>
            <a href="/Admin/CampaignDetail" class="d-sm-inline-block btn btn-sm btn-primary shadow-sm">
                <i class="fas fa-plus fa-sm text-white-50 mr-1"></i> New Campaign
            </a>
        </div>
    </div>


    <div class="row">
        <!-- Default Card Example -->
        <div class="col-xl-6 col-md-12">
            <div class="card mb-4">
                <!-- Card Header - Dropdown -->
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                    <h6 class="m-0 ">Campaign 2021 for APNE Program</h6>
                    <div class="dropdown no-arrow">
                        <a class="dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                           data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <i class="fas fa-ellipsis-v fa-sm fa-fw text-gray-400"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right shadow animated--fade-in"
                             aria-labelledby="dropdownMenuLink">
                            @*<div class="dropdown-header">Dropdown Header:</div>*@
                            <a class="dropdown-item" role="button" onclick="open_popup('editCamp')">Edit</a>
                            <a class="dropdown-item" href="/Admin/Applicants">View Applicants</a>
                            <a class="dropdown-item" role="button" onclick="verify_applicants()">Verify Applicants</a>
                            <a class="dropdown-item" href="#">Export Applicants</a>
                            <a class="dropdown-item" role="button" onclick="open_popup('schedule_meeting')">Schedule Meeting</a>
                            <a class="dropdown-item" role="button" onclick="open_popup('upload_olt')">Upload OLT</a>
                            <a class="dropdown-item" role="button" onclick="open_popup('upload_admitted')">Upload Admitted Applicants</a>

                        </div>
                    </div>
                </div>
                <!-- Card Body -->
                <div class="card-body">
                    <div class="row">
                        <div class="col-12">
                            <!-- Camp Info -->
                            <div class="col-12">
                                <p class="mb-1">
                                    <i class="fas fa-calendar-alt fa-x mr-2 text-gray-400"></i>
                                    Creation Date: <span class="text-gray-500">August 15, 2021</span>
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-calendar-alt fa-x mr-2 text-gray-400"></i>
                                    Registration Open: <span class="text-gray-500">August 22, 2021</span>
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-calendar-alt fa-x mr-2 text-gray-400"></i>
                                    Registration Close: <span class="text-gray-500">Septemper 2, 2021</span>
                                </p>

                                <p class="mb-1 mt-3">
                                    <i class="far fa-check-circle"></i> Interview
                                    <i class="far fa-times-circle ml-3"></i> English Assessment
                                    <i class="far fa-check-circle ml-3"></i> OLT

                                </p>
                                <p class="mb-1 ">
                                    Applications Expire in <span class="text-gray-500">December 31, 2021</span>
                                </p>
                            </div>
                            <!-- Divider -->
                            <hr>

                            <!-- Registration Info -->
                            <div class="col-12">
                                <p class="mb-1">Registration</p>
                                <p class="mb-1">
                                    <i class="fas fa-link fa-x mr-2 text-gray-400"></i>
                                    Registration Link:
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-info-circle fa-x mr-2 text-gray-400"></i>
                                    Form Status: <span class="close-status">close</span>
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-users fa-x mr-1 text-gray-400"></i>
                                    No. of Applicants: <span class="text-gray-500">10685 Persons</span>
                                </p>
                            </div>
                            <!-- Divider -->
                            <hr>

                            <!-- Verification Info -->
                            <div class="col-12">
                                <p class="mb-1">Verification</p>
                                <p class="mb-1">
                                    <i class="fas fa-info-circle fa-x mr-1 text-gray-400"></i>
                                    Verification Status: <span class="open-status">Done</span>
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-user fa-x mr-2 text-gray-400"></i>
                                    Verified By: <span class="text-gray-500">mshehab</span>
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-users fa-x mr-1 text-gray-400"></i>
                                    No. of Qualified: <span class="text-gray-500">6592 Persons</span>
                                </p>
                            </div>

                            <!-- Divider -->
                            <hr>

                            <!-- Rules Acceptance Info -->
                            <div class="col-12">
                                <p class="mb-1">Rules Acceptance </p>

                                <p class="mb-1">
                                    <i class="fas fa-calendar-alt fa-x mr-2 text-gray-400"></i>
                                    Meeting Date: <span class="text-gray-500">--</span>
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-link fa-x mr-2 text-gray-400"></i>
                                    Acceptance Link: <span class="text-gray-500">--</span>
                                </p>
                                <p class="mb-1">
                                    <i class="fas fa-users fa-x mr-1 text-gray-400"></i>
                                    No. of Acceptance: <span class="text-gray-500">--</span>
                                </p>


                            </div>

                            <!-- Divider -->
                            <hr>

                            <!-- Evaluate Info -->
                            <div class="col-12">
                                <p class="mb-1">Applicants Evaluation</p>
                                <p class="mb-1">
                                    <i class="fas fa-users fa-x mr-1 text-gray-400"></i>
                                    No. of Interviews: <span class="text-gray-500">--</span>
                                </p>
